@model IEnumerable<OazachaosuRepository.Model.Article>
@{
  ViewBag.Title = "Blog - Oazachaosu";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var item in Model) {
  @Html.Partial("_ArticlePartial", item);
}

@section left_menu {
  @{
    Html.RenderAction("Menu", "Blog");
  }
}

@section styles{
  <link href="@Url.Content("~/Content/blogStyle.css")" rel="stylesheet" type="text/css">
}